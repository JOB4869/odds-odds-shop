<div id="<%= dom_id order %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Product:</strong>
    <%= order.product_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Buyer:</strong>
    <%= order.buyer_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Seller:</strong>
    <%= order.seller_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= order.status %>
  </div>
</div>
